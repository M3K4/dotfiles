*.font:         Hack-Regular:pixelsize=14,xft:Symbola
*.boldFont:     Hack-Bold:pixelsize=14:weight=bold,xft:Symbola
*.italicFont:   Hack-RegularOblique:pixelsize=14:slant=italic,xft:Symbola
*.alpha: 0.8
! "Color scheme for normal row" Set from: XResources
rofi.color-normal:                   #150506, #f5dfc4, #150506, #CC682D, #150506
! "Color scheme for urgent row" Set from: XResources
rofi.color-urgent:                   #150506, #AC4F24,     #150506, #AC4F24,  #f5dfc4
! "Color scheme for active row" Set from: XResources
rofi.color-active:                   #150506, #f5dfc4, #150506, #CC682D, #150506
! "Color scheme window" Set from: XResources
rofi.color-window:                   #150506, #150506, #CC682D
! "Separator style (none, dash, solid)" Set from: Default
! rofi.separator-style:                dash

